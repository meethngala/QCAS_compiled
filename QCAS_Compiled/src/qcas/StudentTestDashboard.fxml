            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <Button fx:id="LogoutButton" layoutX="533.0" layoutY="110.0" mnemonicParsing="false" onMouseClicked="#logoutStudent" text="Logout">
         <effect>
            <InnerShadow color="#1789b2" />
         </effect>
      </Button>
      <Button fx:id="BackButton" layoutX="506.0" layoutY="366.0" mnemonicParsing="false" onMouseClicked="#goBack" prefHeight="25.0" prefWidth="54.0" text="Back">
         <effect>
            <InnerShadow color="#3f9bb2" />
         </effect>
      </Button>
      <ImageView fitHeight="103.0" fitWidth="613.0" layoutX="-1.0">
         <image>
            <Image url="@../images/red-cmu-display.jpg" />
         </image>
      </ImageView>
