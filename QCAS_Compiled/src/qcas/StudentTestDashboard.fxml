<AnchorPane id="AnchorPane" prefHeight="441.0" prefWidth="600.0" style="-fx-background-color: #eef7f3;" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="qcas.StudentTestDashboardController">
   <children>
      <ComboBox id="TypeofQuestionComboBox" fx:id="studentTComboBox" layoutX="44.0" layoutY="196.0" prefHeight="25.0" prefWidth="172.0" promptText="Number of Questions" visibleRowCount="4" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="7.0" layoutY="246.0" prefHeight="47.0" prefWidth="324.0" text="Choose the difficulty level">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <RadioButton id="EasyRadioButton" fx:id="RB1" layoutX="44.0" layoutY="293.0" mnemonicParsing="false" text="Easy">
         <toggleGroup>
            <ToggleGroup fx:id="studentTG" />
         </toggleGroup></RadioButton>
      <RadioButton id="MediumRadioButton" fx:id="RB2" layoutX="43.0" layoutY="319.0" mnemonicParsing="false" text="Medium" toggleGroup="$studentTG" />
      <RadioButton id="HardRadioButton" fx:id="RB3" layoutX="43.0" layoutY="344.0" mnemonicParsing="false" text="Hard" toggleGroup="$studentTG" />
      <Button id="startTestButton" fx:id="startTestButton" layoutX="379.0" layoutY="228.0" mnemonicParsing="false" onMouseClicked="#goToTest" prefHeight="47.0" prefWidth="181.0" style="-fx-background-color: #ADD8E6; -fx-background-radius: 10;" text="Start Test!">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
